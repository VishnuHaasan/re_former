<%= form_with model: @user, method: :patch do |form|%>
  <%= form.label :username, "Username: " %>
  <%= form.text_field :username,value: "Your username here" %><br>
  <%= form.label :password, "Password: " %>
  <%= form.text_field :password,value: "Your password here" %><br>
  <%= form.label :email, "Email: " %>
  <%= form.text_field :email,value: "Your email here" %><br>
  <%= form.submit %>
<% end %>

<%= link_to "Back to root path.",root_path %>