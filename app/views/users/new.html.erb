<%= form_with model: @user, method: :post do |form|%>
  <%= form.label :username, "Username: " %>
  <%= form.text_field :username,value: "Your username here" %>
  <%= form.label :password, "Password: " %>
  <%= form.text_field :password,value: "Your password here" %>
  <%= form.label :email, "Email: " %>
  <%= form.text_field :email,value: "Your email here" %>
  <%= form.submit %>
<% end %>
